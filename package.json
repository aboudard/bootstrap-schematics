{
  "name": "@aboudard/bootstrap-schematics",
  "version": "1.0.3",
  "description": "Angular schematics for bootstrap",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aboudard/bootstrap-schematic.git"
  },
  "bugs": {
    "url": "https://github.com/aboudard/bootstrap-schematic/issues"
  },
  "homepage": "https://github.com/aboudard/bootstrap-schematic",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "link:schematic": "npm link && cd sandbox && npm link \"bootstrap-schematics\"",
    "sandbox:ng-add": "cd sandbox && ng g bootstrap-schematics:ng-add",
    "clean": "git checkout HEAD -- sandbox && git clean -f -d sandbox",
    "test": "npm run clean && npm run sandbox:ng-add && npm run test:sandbox",
    "test:unit": "npm run build && jasmine src/**/*_spec.js",
    "test:sandbox": "cd sandbox && npm run lint && npm run test && npm run build",
    "publish": "npm run build && npm publish"
  },
  "keywords": [
    "schematics"
  ],
  "author": "aboudard",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^9.0.4",
    "@angular-devkit/schematics": "^9.0.4",
    "typescript": "~3.7.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "jasmine": "^3.5.0"
  }
}